{
  "$schema": "http://json.schemastore.org/template",
  "author": "Juan Daniel Arce",
  "classifications": ["Web"],
  "identity": "solidaria.ArquitecturaLimpia",
  "name": "Arquitectura Limpia",
  "description": "Arquitectura limpia para los servicios backend de aseguradora solidaria",
  "shortName": "arquitecturalimpia",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "TemplateSolidaria",
  "preferNameDirectory": true,
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "El framework sobre el que se va a construir la aplicaci√≥n",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net6.0"
        },
        {
          "choice": "net7.0"
        }
      ],
      "defaultValue": "net6.0",
      "replaces": "{TargetFramework}"
    },
    "SqlServerAdapter": {
      "type": "parameter",
      "description": "Adaptador de base de datos para SQL Server",
      "datatype": "bool",
      "defaultValue": "false"
    }
  },
  "sources": [
    {
      "exclude": [
        "**/[Bb]in/**",
        "**/[Oo]bj/**",
        ".template.config/**/*",
        "**/*.filelist",
        "**/*.user",
        "**/*.lock.json",
        "**/*.log",
        ".git/",
        ".vs/"
      ]
    }
  ]
}
